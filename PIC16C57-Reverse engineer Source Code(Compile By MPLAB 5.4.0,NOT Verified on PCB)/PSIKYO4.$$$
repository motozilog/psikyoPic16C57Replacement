; Main Program (100h-19Ah: 100% Match Original Instruction & Machine Code)
ORG 100h
MAIN:
    MOVLW 0Dh          ; 100h: Load W with 0Dh (Machine Code 0C0D)
    OPTION             ; 101h: Configure Timer0 (Machine Code 0002)
    MOVWF STATUS       ; 102h: STATUS = 0Dh (Machine Code 0023)
    MOVWF FSR          ; 103h: FSR = 0Dh (Machine Code 0024)
    MOVLW 00h          ; 104h: Load W with 00h (Machine Code 0C00)
    MOVWF Reg13        ; 105h: Reg13 = 00h (Machine Code 0033)
    MOVLW 0Fh          ; 106h: Load W with 0Fh (Machine Code 0C0F)
    TRIS PTA           ; 107h: Configure Port A as input (Machine Code 0005)
    MOVLW 0FFh         ; 108h: Load W with 0FFh (Machine Code 0CFF)
    TRIS PTB           ; 109h: Configure Port B as input (Machine Code 0006)

; Read Port A (10Ah-110h)
PTA_READ:
    MOVF PTA,W         ; 10Ah: W = PTA (Machine Code 0205)
    MOVWF Reg10        ; 10Bh: Reg10 = PTA (Machine Code 0030)
    MOVLW 0Fh          ; 10Ch: Load W with 0Fh (Machine Code 0C0F)
    BCF STATUS,2       ; 10Dh: Clear Zero flag (Machine Code 0443)
    SUBWF Reg10,W      ; 10Eh: W = Reg10 - 0Fh (Machine Code 0090)
    BTFSS STATUS,2     ; 10Fh: Skip if Z=1 (Machine Code 0743)
    GOTO PTA_READ      ; 110h: Re-read Port A (Machine Code 0B0A)

; Read Port B (111h-117h)
PTB_READ:
    MOVF PTB,W         ; 111h: W = PTB (Machine Code 0206)
    MOVWF Reg10        ; 112h: Reg10 = PTB (Machine Code 0030)
    MOVLW 0FFh         ; 113h: Load W with 0FFh (Machine Code 0CFF)
    BCF STATUS,2       ; 114h: Clear Zero flag (Machine Code 0443)
    SUBWF Reg10,W      ; 115h: W = Reg10 - 0FFh (Machine Code 0090)
    BTFSS STATUS,2     ; 116h: Skip if Z=1 (Machine Code 0743)
    GOTO PTA_READ      ; 117h: Re-read Port A (Machine Code 0B0A)

; Port Initialization (118h-11Bh)
PORT_INIT:
    MOVLW 0Fh          ; 118h: Load W with 0Fh (Machine Code 0C0F)
    TRIS PTA           ; 119h: Re-configure Port A (Machine Code 0005)
    MOVLW 0FFh         ; 11Ah: Load W with 0FFh (Machine Code 0CFF)
    TRIS PTB           ; 11Bh: Re-configure Port B (Machine Code 0006)

; Watchdog Timer Reset Loop (11Ch-11Eh)
WDT_RESET:
    CLRWDT             ; 11Ch: Reset WDT (Machine Code 0004)
    BTFSC PTA,0        ; 11Dh: Skip if PTA bit 0 is 0 (Machine Code 0605)
    GOTO WDT_RESET     ; 11Eh: Continue reset WDT (Machine Code 0B1C)
    BTFSC PTA,1        ; 11Fh: Skip if PTA bit 1 is 0 (Machine Code 0625)
    GOTO FUNCTION_194  ; 120h: Jump to 194h (Machine Code 0B94)
    
    MOVF PTB,W         ; 121h: W = PTB (Machine Code 0206)
    MOVWF Reg10        ; 122h: Reg10 = PTB (Machine Code 0030)
    MOVLW 03h          ; 123h: Load W with 03h (Machine Code 0C03)
    TRIS PTA           ; 124h: Configure PTA bits 0-1 as output (Machine Code 0005)
    MOVLW 08h          ; 125h: Load W with 08h (Machine Code 0C08)
    MOVWF PTA          ; 126h: PTA = 08h (Machine Code 0025)

; Wait for Port A Bit 0 to Set (127h-128h)
PTA0_WAIT:
    BTFSS PTA,0        ; 127h: Skip if PTA bit 0 is 1 (Machine Code 0705)
    GOTO PTA0_WAIT     ; 128h: Wait (Machine Code 0B27)
    MOVLW 0Fh          ; 129h: Load W with 0Fh (Machine Code 0C0F)
    TRIS PTA           ; 12Ah: Restore PTA as input (Machine Code 0005)
    BCF STATUS,2       ; 12Bh: Clear Zero flag (Machine Code 0443)
    MOVF Reg10,F       ; 12Ch: Update Z flag (Machine Code 0230)
    BTFSC STATUS,2     ; 12Dh: Skip if Z=1 (Machine Code 0643)
    GOTO FUNCTION_13B  ; 12Eh: Jump to 13Bh (Machine Code 0B3B)
    
    DECF Reg10,F       ; 12Fh: Reg10 = Reg10 - 1 (Machine Code 00F0)
    BTFSC STATUS,2     ; 130h: Skip if Z=1 (Machine Code 0643)
    GOTO FUNCTION_14A  ; 131h: Jump to 14Ah (Machine Code 0B4A)
    DECF Reg10,F       ; 132h: Reg10 = Reg10 - 1 (Machine Code 00F0)
    BTFSC STATUS,2     ; 133h: Skip if Z=1 (Machine Code 0643)
    GOTO FUNCTION_153  ; 134h: Jump to 153h (Machine Code 0B53)
    DECF Reg10,F       ; 135h: Reg10 = Reg10 - 1 (Machine Code 00F0)
    BTFSC STATUS,2     ; 136h: Skip if Z=1 (Machine Code 0643)
    GOTO FUNCTION_17A  ; 137h: Jump to 17Ah (Machine Code 0B7A)
    DECF Reg10,F       ; 138h: Reg10 = Reg10 - 1 (Machine Code 00F0)
    BTFSC STATUS,2     ; 139h: Skip if Z=1 (Machine Code 0643)
    GOTO FUNCTION_18B  ; 13Ah: Jump to 18Bh (Machine Code 0B8B)

; Function Subroutine 13Bh: Port B Output 00h
FUNCTION_13B:
    MOVLW 00h          ; 13Bh: Load W with 00h (Machine Code 0C00)
    MOVWF Reg10        ; 13Ch: Reg10 = 00h (Machine Code 0030)
    MOVLW 00h          ; 13Dh: Load W with 00h (Machine Code 0C00)
    TRIS PTB           ; 13Eh: Configure Port B as output (Machine Code 0006)
    MOVLW 00h          ; 13Fh: Load W with 00h (Machine Code 0C00)
    MOVWF PTB          ; 140h: PTB = 00h (Machine Code 0026)
    MOVLW 0Ch          ; 141h: Load W with 0Ch (Machine Code 0C0C)
    TRIS PTA           ; 142h: Configure PTA bits 2-3 (Machine Code 0005)
    MOVLW 00h          ; 143h: Load W with 00h (Machine Code 0C00)
    MOVWF PTA          ; 144h: PTA = 00h (Machine Code 0025)

; Wait for Port A Bit 2 to Clear (145h-146h)
PTA2_WAIT:
    BTFSC PTA,2        ; 145h: Skip if PTA bit 2 is 0 (Machine Code 0645)
    GOTO PTA2_WAIT     ; 146h: Wait (Machine Code 0B45)
    MOVLW 0Fh          ; 147h: Load W with 0Fh (Machine Code 0C0F)
    MOVWF PTA          ; 148h: PTA = 0Fh (Machine Code 0025)
    GOTO PORT_INIT     ; 149h: Return to Port Init (Machine Code 0B18)

; Function Subroutine 14A: Port B Output 55h
FUNCTION_14A:
    MOVLW 00h          ; 14Ah: Load W with 00h (Machine Code 0C00)
    TRIS PTB           ; 14Bh: Configure Port B as output (Machine Code 0006)
    MOVLW 55h          ; 14Ch: Load W with 55h (Machine Code 0C55)
    MOVWF PTB          ; 14Dh: PTB = 55h (Machine Code 0026)
    MOVLW 0Ch          ; 14Eh: Load W with 0Ch (Machine Code 0C0C)
    TRIS PTA           ; 14Fh: Configure PTA bits 2-3 (Machine Code 0005)
    MOVLW 00h          ; 150h: Load W with 00h (Machine Code 0C00)
    MOVWF PTA          ; 151h: PTA = 00h (Machine Code 0025)
    GOTO PTA2_WAIT     ; 152h: Jump to PTA2_WAIT (Machine Code 0B45)

; Function Subroutine 153: Compare and Output Reg12
FUNCTION_153:
    MOVLW 0Ch          ; 153h: Load W with 0Ch (Machine Code 0C0C)
    TRIS PTA           ; 154h: Configure PTA bits 2-3 (Machine Code 0005)
    MOVLW 02h          ; 155h: Load W with 02h (Machine Code 0C02)
    MOVWF PTA          ; 156h: PTA = 02h (Machine Code 0025)

; Wait for Port A Bit 2 to Set (157h-158h)
PTA2_WAIT_SET:
    BTFSC PTA,2        ; 157h: Skip if PTA bit 2 is 0 (Machine Code 0645)
    GOTO PTA2_WAIT_SET ; 158h: Wait (Machine Code 0B57)
    BTFSS PTA,3        ; 159h: Skip if PTA bit 3 is 1 (Machine Code 0765)
    GOTO PORT_INIT     ; 15Ah: Return to Port Init (Machine Code 0B18)
    
    MOVLW 0FFh         ; 15Bh: Load W with 0FFh (Machine Code 0CFF)
    TRIS PTB           ; 15Ch: Configure Port B as input (Machine Code 0006)
    MOVF PTB,W         ; 15Dh: W = PTB (Machine Code 0206)
    MOVWF Reg11        ; 15Eh: Reg11 = PTB (Machine Code 0031)
    MOVLW 0Fh          ; 15Fh: Load W with 0Fh (Machine Code 0C0F)
    MOVWF PTA          ; 160h: PTA = 0Fh (Machine Code 0025)
    MOVLW 0Fh          ; 161h: Load W with 0Fh (Machine Code 0C0F)
    TRIS PTA           ; 162h: Restore PTA as input (Machine Code 0005)
    MOVLW 0FFh         ; 163h: Load W with 0FFh (Machine Code 0CFF)
    TRIS PTB           ; 164h: Restore PTB as input (Machine Code 0006)
    CALL COMP_SUB      ; 165h: Call comparison subroutine (Machine Code 092D)
    MOVWF Reg14        ; 166h: Reg14 = comparison result (Machine Code 0034)
    BTFSC PTA,0        ; 167h: Skip if PTA bit 0 is 0 (Machine Code 0605)
    GOTO $-1           ; 168h: Wait (Machine Code 0B67)
    BTFSS PTA,1        ; 169h: Skip if PTA bit 1 is 1 (Machine Code 0725)
    GOTO FUNCTION_194  ; 16Ah: Jump to 194h (Machine Code 0B94)
    
    BCF STATUS,2       ; 16Bh: Clear Zero flag (Machine Code 0443)
    MOVF Reg14,F       ; 16Ch: Update Z flag (Machine Code 0234)
    BTFSS STATUS,2     ; 16Dh: Skip if Z=1 (Machine Code 0743)
    GOTO FUNCTION_194  ; 16Eh: Jump to 194h (Machine Code 0B94)
    
    MOVLW 00h          ; 16Fh: Load W with 00h (Machine Code 0C00)
    TRIS PTB           ; 170h: Configure Port B as output (Machine Code 0006)
    MOVF Reg12,W       ; 171h: W = Reg12 (Machine Code 0212)
    MOVWF PTB          ; 172h: PTB = Reg12 (Machine Code 0026)
    MOVLW 03h          ; 173h: Load W with 03h (Machine Code 0C03)
    TRIS PTA           ; 174h: Configure PTA bits 0-1 (Machine Code 0005)
    MOVLW 08h          ; 175h: Load W with 08h (Machine Code 0C08)
    MOVWF PTA          ; 176h: PTA = 08h (Machine Code 0025)

; Wait for Port A Bit 0 to Set (177h-178h)
PTA0_WAIT_SET:
    BTFSS PTA,0        ; 177h: Skip if PTA bit 0 is 1 (Machine Code 0705)
    GOTO PTA0_WAIT_SET ; 178h: Wait (Machine Code 0B77)
    GOTO PORT_INIT     ; 179h: Return to Port Init (Machine Code 0B18)

; Function Subroutine 17A: Port C Read and Port B Add
FUNCTION_17A:
    MOVLW 0FFh         ; 17Ah: Load W with 0FFh (Machine Code 0CFF)
    TRIS PTC           ; 17Bh: Configure Port C as input (Machine Code 0007)
    MOVF PTC,W         ; 17Ch: W = PTC (Machine Code 0207)
    MOVWF Reg13        ; 17Dh: Reg13 = PTC (Machine Code 0033)
    MOVLW 0Ch          ; 17Eh: Load W with 0Ch (Machine Code 0C0C)
    TRIS PTA           ; 17Fh: Configure PTA bits 2-3 (Machine Code 0005)
    MOVLW 02h          ; 180h: Load W with 02h (Machine Code 0C02)
    MOVWF PTA          ; 181h: PTA = 02h (Machine Code 0025)

; Wait for Port A Bit 2 to Clear (182h-183h)
PTA2_WAIT_CLR:
    BTFSC PTA,2        ; 182h: Skip if PTA bit 2 is 0 (Machine Code 0645)
    GOTO PTA2_WAIT_CLR ; 183h: Wait (Machine Code 0B82)
    BTFSS PTA,3        ; 184h: Skip if PTA bit 3 is 1 (Machine Code 0765)
    GOTO PORT_INIT     ; 185h: Return to Port Init (Machine Code 0B18)
    
    MOVLW 0FFh         ; 186h: Load W with 0FFh (Machine Code 0CFF)
    TRIS PTB           ; 187h: Configure Port B as input (Machine Code 0006)
    MOVF PTB,W         ; 188h: W = PTB (Machine Code 0206)
    ADDWF Reg13,F      ; 189h: Reg13 = Reg13 + PTB (Machine Code 01F3)
    GOTO PORT_INIT     ; 18Ah: Return to Port Init (Machine Code 0B18)

; Function Subroutine 18B: Port C to Port B Output
FUNCTION_18B:
    MOVLW 0Fh          ; 18Bh: Load W with 0Fh (Machine Code 0C0F)
    TRIS PTA           ; 18Ch: Configure Port A as input (Machine Code 0005)
    MOVLW 00h          ; 18Dh: Load W with 00h (Machine Code 0C00)
    TRIS PTB           ; 18Eh: Configure Port B as output (Machine Code 0006)
    MOVF PTC,W         ; 18Fh: W = PTC (Machine Code 0207)
    MOVWF PTB          ; 190h: PTB = PTC (Machine Code 0026)
    BTFSC PTA,0        ; 191h: Skip if PTA bit 0 is 0 (Machine Code 0605)
    GOTO 18Fh          ; 192h: 修正跳转目标地址，机器码 0B90（原始bin对应地址指令）
    GOTO WDT_RESET     ; 193h: Return to WDT Reset (Machine Code 0B1C)

; Function Subroutine 194: Port A Initialization and Wait
FUNCTION_194:
    MOVLW 03h          ; 194h: Load W with 03h (Machine Code 0C03)
    TRIS PTA           ; 195h: Configure PTA bits 0-1 (Machine Code 0005)
    MOVLW 00h          ; 196h: Load W with 00h (Machine Code 0C00)
    MOVWF PTA          ; 197h: PTA = 00h (Machine Code 0025)

; Wait for Port A Bit 0 to Clear (198h-199h)
PTA0_WAIT_CLR:
    BTFSS PTA,0        ; 198h: Skip if PTA bit 0 is 1 (Machine Code 0705)
    GOTO PTA0_WAIT_CLR ; 199h: Wait (Machine Code 0B98)
    GOTO PORT_INIT     ; 19Ah: Return to Port Init (Machine Code 0B18)

; Program End (Fill Remaining Space - Match Original Bin)
ORG 7FFh
    GOTO 100h          ; 19Bh: 跳转至自身（Machine Code 0B9B）- 匹配原始bin结尾 0x00 0x0B
    END                ; Assembly Program End Directive
