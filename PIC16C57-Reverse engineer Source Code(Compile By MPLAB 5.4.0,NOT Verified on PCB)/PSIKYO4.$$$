; Comparison Subroutine (02Dh-037h: Match Original Instruction Sequence)
ORG 02Dh
COMP_SUB:
    MOVLW 2Ch          ; 02Dh: Load W with 2Ch (Machine Code 0C2C)
    MOVWF Reg10        ; 02Eh: Store W to Reg10 (Machine Code 0030)
    MOVF Reg13,W       ; 02Fh: Load W with Reg13 (Machine Code 0213)
    SUBWF Reg11,W      ; 030h: W = Reg11 - Reg13 (Machine Code 0091)
    BCF STATUS,0       ; 031h: Clear Carry flag (Machine Code 0403)
    SUBWF Reg10,F      ; 032h: Reg10 = Reg10 - W (Machine Code 00B0)
    BTFSS STATUS,0     ; 033h: Skip if Carry set (Machine Code 0703)
    RETLW 01h          ; 034h: Return 01h (Machine Code 0801)
    CALL 000h          ; 035h: Call 000h (Machine Code 0900)
    MOVWF Reg12        ; 036h: Store W to Reg12 (Machine Code 0032)
    RETLW 00h          ; 037h: Return 00h (Machine Code 0800)

